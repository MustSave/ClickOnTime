{
  "manifest_version": 2,
  "name": "특정 시간에 클릭",
  "version": "1.0",
  "permissions": ["tabs", "activeTab", "*://*/*"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"]
    }
  ]
}
